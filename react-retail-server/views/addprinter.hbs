 <style style="text/css">
        .content-wrapper{ padding-left: 15px; padding-right:15px;}
        
        .dataTables_wrapper .dataTables_paginate .paginate_button{ padding:0px !important;}
        .error{
            color:red;
        }
    </style>
<div class="content-wrapper pb-4">

    <div class="row">
        <!-- data table start -->
        <div class="col-12 mt-1">
            <div class="card">

                <div class="card-header">
                    <div class="left d-inline-block">
                        <h4 class="mb-0"> <i class="ti-stats-up" style="color:#000"></i> Add Printer</h4>
                        <p class="mb-0 dateTime"></p>
                    </div>
                    <div class="right d-inline-block float-right mt-2">
                        <img src="/assets/images/count/Group 9.png" height="30px">
                        <img src="/assets/images/count/Icon feather-printer.png" height="30px" class="ml-1 mr-1">
                    </div>
                </div>
                <div class="card-body">
                    <form autocomplete="off" id="AddPrinterForm" name="AddPrinterForm">
                       <span class="error_msg"></span>
                        <div class="form-row">
                            <div class="form-group col-md-6">
                                <label for="Name">Name *</label>
                                <input type="text" class="form-control" name="name" id="Name" placeholder="Name">
                                <span class="error error_name"></span>
                            </div>
                            <div class="form-group col-md-6">
                              <label for="status">Status *</label>
                              <select class="form-control selectpicker" name="status" id="Status" data-live-search="true"  name="roles">
                                    <option value="1">Active</option>
                                    <option value="0">Disable</option>
                                </select>
                                <span class="error error_status"></span>
                            </div>
                            

                            <div class="form-group col-md-6">
                              <label for="status">Remarks *</label>
                                <input type="text" class="form-control" name="remarks" id="remarks" placeholder="Remarks">
                                <span class="error error_remarks"></span>
                            </div>

                            <div class="form-group col-md-6">
                              <label for="ip">IP *</label>
                                <input type="text" class="form-control" name="ip" id="ip" placeholder="IP">
                                <span class="error error_ip"></span>
                            </div>

                            <div class="form-group col-md-6">
                              <label for="port">Port *</label>
                                <input type="text" class="form-control" name="port" id="port" placeholder="Port">
                                <span class="error error_port"></span>
                            </div>
                            <div class="form-group col-md-6">
                                <label for="storename">Storename *</label>
                                <select class="selectpicker form-control" data-live-search="true"  name="stoid" id="StoreID" multiple>
                                    <option value="">Store ID</option>
                                </select>
                                <span class="error error_stoid"></span>
                            </div>
  
                            
                            <div class="form-group  col-md-12" style="text-align:center;">
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="but mt-5">
                                            <button type="submit" id="submit" class="btn btn-default btn-block">   
                                             Add Printer
                                            </button>
                                        </div>   
                                    </div>
                                </div>  
                                  
                            </div>
                               
                        </div>
                    </form>
                </div>
            </div>
        </div>
        
        <!-- Dark table end -->
    </div>
</div>        