<style style="text/css">
    .content-wrapper{ padding-left: 15px; padding-right:15px;}
    .supplychaindashboardSubmenudropdown{display: block;}
    .dataTables_wrapper .dataTables_paginate .paginate_button{ padding:0px !important;}
</style>
<div class="content-wrapper pb-4">

    <div class="header-sec" style="margin-left: -15px;">
        <nav class="navbar navbar-expand-lg navbar-light p-0 d-inline-block">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item">
                    <a class="nav-link" href="supplychaindashboard">
                        <img src="/assets/images/dashboard-icon.png" height="20px">
                        <span class="ml-2 nav-text text-uppercase">Dashboard</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link active2" href="asndata">
                        <img src="/assets/images/count/Icon ionic-ios-flower.png" height="20px">
                        <span class="ml-2 nav-text text-uppercase">Transfer Cancelation</span>
                    </a>
                </li>
            </ul>
        </nav>
    </div>

    <div class="row">
        <!-- data table start -->
        <div class="col-12 mt-1">
            <div class="card">

                <div class="card-header">
                    <div class="left d-inline-block">
                        <h4 class="mb-0"> 
                            <i class="ti-stats-up" style="color:#000"></i> Cancel Asn Data</h4>
                        <p class="mb-0 dateTime"></p>
                    </div>
                    <div class="right d-inline-block float-right mt-2">
                        <img src="/assets/images/count/Group 9.png" height="30px">
                        <img src="/assets/images/count/Icon feather-printer.png" height="30px" class="ml-1 mr-1">
                        
                       <span style="cursor:pointer" onclick="window.location.href='{{sitelink}}/api/1.0.0/stockCountRecords/getasndata_ex/'">
                            <img src="/assets/images/count/Group 10.png" height="30px">
                        </span>
                    
                    </div>
                </div>
                <div class="card-body">
                    <div class="containe">
                        <div class="row">
                            <div class="col-md-6 mt-1">
                                <form>
                                    <div class="form-group">
                                        <label for="Company">From Sotre ID</label>
                                        <select name="StoreID" 
                                        class="form-control selectpicker" 
                                        id="StoreID" data-live-search="true">
                                            <option value="0">Type</option>
                                        </select>
                                    </div>
                                    
                                    <div class="form-group">
                                        <label for="Date">Date</label>
                                        <input type="date" class="form-control" 
                                        style="color: #4C9FE1 !important;" id="Date" 
                                         pattern="\d{4}-\d{2}-\d{2}">
                                    </div>

                                    <div class="form-group">
                                        <label for="SerialNumber">Serial Number</label>
                                        <input type="text" class="form-control" 
                                        style="color: #4C9FE1 !important;" id="SerialNumber">
                                    </div>

                                    
                                    
                                </form>
                            </div>
                            <div class="col-md-6 mt-1">
                                <form>
                                    <div class="form-group">
                                        <label for="Process">Process</label>
                                        <select name="Process" 
                                        class="form-control selectpicker" 
                                        id="Process" data-live-search="true">
                                            <option value="0">Process</option>
                                        </select>
                                    </div>

                                    <div class="form-group">
                                        <label for="EPC">ASN</label>
                                            <input type="text" class="form-control" 
                                            id="Asn" style="color: #4C9FE1 !important;">
                                    </div>

                                    <div class="form-group col-6" style="margin-left: -15px;">
                                      <label>
                                      </label>
                                        <button type="button" 
                                        class="btn btn-danger btn-md run ml-2 mt-4 btn-block" 
                                        style="margin-top: 9px !important">Search</button>
                                    </div>
                                </form>

                            </div>
                        </div>
                    </div>
                    <div class="data-tables">
                        <table id="dataTable" class="text-center mm-datatable">
                            <thead class="bg-light text-capitalize">
                                <tr>
                                    <th>Id</th>
                                    <th>Date</th>
                                    <th>Asn</th>
                                    <th>Process</th>
                                    <th>Department</th>
                                    <th>Destination</th>
                                    <th>Style</th>
                                    <th>Color</th>
                                    <th>Brand</th> 
                                    <th>Action</th>
                                    {{!-- <th>Purchase Order</th>
                                    <th>User</th>  --}}                                          
                                </tr>
                            </thead>
                            <tbody>

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        

        <!-- Dark table end -->
    </div>
</div>    
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" 
    aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content" style="width:145%">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Permissions</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <form  id="UserRoles">
                <style type="text/css" media="screen">
                    .modal-body .load{
                        text-align: center;
                    }
                </style>
                <div class="edit-manage"></div>
                <div class="modal-body" style="max-height: 560px">
                    <div class="load">
                        <span class="waiting" style="display: none;">
                        <img src="https://i.postimg.cc/3xVJyMYr/Eclipse-1s-200px.gif" alt="" width='32px' height='32px'>
                    </span>   
                    </div>
                     
                   <div id="permissions_list">
                   
                   </div> 
                   <div id="ProcessTable">
                   
                   </div>
                   <div id="zone">
                   
                   </div>                         
                </div>
            </form>
        </div>
    </div>
</div>
